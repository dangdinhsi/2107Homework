<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
         version="2.5">
    <servlet>
        <servlet-name>HelloAPI</servlet-name>
        <servlet-class>endpoints.HelloAPI</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>HelloAPI</servlet-name>
        <url-pattern>/music</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>CreateStudent</servlet-name>
        <servlet-class>controller.CreateStudent</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CreateStudent</servlet-name>
        <url-pattern>/student/create</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>ListStudent</servlet-name>
        <servlet-class>controller.ListStudent</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ListStudent</servlet-name>
        <url-pattern>/student/list</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>DetailStudent</servlet-name>
        <servlet-class>controller.DetailStudent</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DetailStudent</servlet-name>
        <url-pattern>/student/detail</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>DeleteStudent</servlet-name>
        <servlet-class>controller.DeleteStudent</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DeleteStudent</servlet-name>
        <url-pattern>/student/delete</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>EditStudent</servlet-name>
        <servlet-class>controller.EditStudent</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>EditStudent</servlet-name>
        <url-pattern>/student/edit</url-pattern>
    </servlet-mapping>
   <servlet>
       <servlet-name>PublisherPubSub</servlet-name>
       <servlet-class>pubsub.PublisherDemo</servlet-class>
   </servlet>
    <servlet-mapping>
        <servlet-name>PublisherPubSub</servlet-name>
        <url-pattern>/postpub</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>Sub01</servlet-name>
        <servlet-class>pubsub.SubscriberDemo</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Sub01</servlet-name>
        <url-pattern>/sub01</url-pattern>
    </servlet-mapping>
    <filter>
        <filter-name>ObjectifyFilter</filter-name>
        <filter-class>com.googlecode.objectify.ObjectifyFilter</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>ObjectifyFilter</filter-name>
        <url-pattern>/*</url-pattern>
    </filter-mapping>
    <filter>
        <filter-name>ObjectifyRegister</filter-name>
        <filter-class>filter.ObjectifyRegister</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>ObjectifyRegister</filter-name>
        <url-pattern>/*</url-pattern>
    </filter-mapping>
</web-app>